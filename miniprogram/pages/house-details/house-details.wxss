.weui-panel {
  background-color: #fff;
  margin-top: 10px;
  position: relative;
  overflow: hidden;
  font-size: 2.4vw;
}

.weui-panel__hd {
  padding: 16px 16px 13px;
  color: rgba(0, 0, 0, 0.9);
  font-size: 15px;
  font-weight: 700;
  position: relative;
}

.weui-panel__hd:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  color: var(--weui-FG-3);
  transform-origin: 0 100%;
  transform: scaleY(0.5);
  left: 15px;
}


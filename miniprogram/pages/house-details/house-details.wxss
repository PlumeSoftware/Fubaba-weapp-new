  overflow: hidden;
}

.baidumap_wrapper {
  height: 300px;
}


.house {
  padding-top: 10px;
  padding-bottom: 0;
  background-color: #fff;
}

.weui-article {
  padding: 10px 16px 0px 16px;
  font-size: 17px;
  color: rgba(0, 0, 0, 0.9);
  word-wrap: break-word;
  -webkit-hyphens: auto;
  hyphens: auto;
}

.house-tags {
  margin: 0;
  /* padding-top: 16px; */
  overflow: hidden;
}

.house .house-tags .tag {
  display: inline-block;
  font-size: 12px;
  font-weight: 700;
  letter-spacing: 0;
  height: 16px;
  line-height: 16px;
  color: #849aae;
  border-radius: 0.533vw;
  padding: 0 3px;
  background: #edf0f3;
  margin-right: 5px;
  margin-bottom: 3px;
}

.house-overview-desc {
  position: relative;
  margin-top: 10px;
}

.house .house-overview-desc .house-title {
  font-size: 4.8vw;
  margin: 0 44px 0 0;
}

.weui-article h1 {
  font-size: 22px;
  font-weight: 700;
  margin-bottom: 0.9em;
  line-height: 1.4;
}

.house .house-overview-desc .attention {
  position: absolute;
  width: 44px;
  right: 0;
  top: 50%;
  font-size: 0;
  transform: translateY(-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
}

.attention .attention-icon {
  margin: 0 2.667vw;
  display: inline-block;
  width: 7.867vw;
  height: 7.867vw;
  background: url(data:image/svg+xml;base64,);
  background-size: 7.867vw 7.867vw;
  background-repeat: no-repeat;
}

.text {
  display: block;
  color: #101d37;
  font-size: 3.667vw;
  height: 3.667vw;
  line-height: 2.667vw;
  text-align: center;
  font-weight: 500;
}

.house-detail-desc {
  width: 100%;
  margin-top: 16px;
  font-size: 3.8vw;
}

.house .house-detail-desc .desc-block .desc {
  font-weight: 700;
  color: #fa5741;
  letter-spacing: 0;
  line-height: 5.067vw;
  margin: 0;
}

.house .house-detail-desc .desc-block .title {
  /* display: inline-block; */
  font-size: 3.4vw;
  color: #999;
  letter-spacing: 0;
  line-height: 2.933vw;
  margin: 1.067vw 0 0;
}

.weui-article p {
  margin: 0 0 0.8em;
}

.house section:last-child {
  margin-bottom: 0;
}

.house .house-intro {
  margin-top: 10px;
  background: #fff;
  overflow: hidden;
}

.house-overview {
  margin-bottom: 1.5em;
}

.house .house-intro .weui-flex.house-attrs {
  display: flex;
  flex-wrap: wrap;
}

.weui-flex {
  display: flex;
}

.house .house-intro .weui-flex.house-attrs .weui-flex__item {
  flex: 1 0 50%;
  margin-bottom: 12px;
}

.weui-flex__item {
  flex: 1;
}

.house .house-intro .weui-flex.house-attrs .weui-flex__item .attr {
  font-size: 3.8vw;
  color: #999;
  letter-spacing: 0;
  line-height: 4.267vw;
}

.house .house-intro .weui-flex.house-attrs .weui-flex__item .desc {
  font-size: 3.8vw;
  color: #222;
  letter-spacing: 0;
}

.house .house-intro .weui-flex.house-attrs {
  display: flex;
  flex-wrap: wrap;
}

.agent-foot {
  position: fixed;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  height: 21.333vw;
  padding: 4.267vw 3.1vw;
  background: #fff;
  box-shadow: 0 -0.0625rem 0.125rem rgba(0, 0, 0, 0.1);
  z-index: 9;
}

.agent-foot .agent-container {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.agent-foot .agent-container .agent-info {
  width: 40%;
  padding-top: 0.2rem;
  overflow: hidden;
  position: relative;
  display: flex;
  justify-content: start;
}

.agent-foot .agent-container .agent-info .agent-avatar-block {
  position: relative;
  margin: 0;
  padding-bottom: 0.3rem;
  width: 40px;
  width: 2.5rem;
  height: 40px;
  height: 2.5rem;
}

.agent-foot .agent-container .agent-info .agent-avatar-block .agent-avatar {
  width: 2.5rem;
  height: 2.5rem;
  margin-right: 0.75rem;
  position: relative;
  border-radius: 100%;
  overflow: hidden;
}

a {
  text-decoration: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.agent-foot .agent-container .agent-info .agent-avatar-block .agent-avatar img {
  width: 100%;
  height: auto;
}

a img {
  border: 0;
}

.agent-foot .agent-container .agent-info .agent-des {
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 0;
}

.agent-foot .agent-container .agent-info .agent-des .agent-name {
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #101d37;
  font-size: 16px;
  font-weight: 700;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  max-width: 80px;
  margin-bottom: 6px;
}

.agent-foot .agent-container .agent-content {
  width: 60%;
}

.weui-btn_primary {
  background-color: var(--weui-BRAND);
}

.weui-btn {
  position: relative;
  display: block;
  width: 184px;
  margin-left: auto;
  margin-right: auto;
  padding: 8px 24px;
  box-sizing: border-box;
  font-weight: 700;
  font-size: 17px;
  text-align: center;
  text-decoration: none;
  color: #fff;
  line-height: 1.41176471;
  border-radius: 4px;
  overflow: hidden;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.sideicon {
  position: fixed;
  right: 15px;
  bottom: 6.4vw;
  z-index: 999;
  width: 12.8vw;
  height: 12.8vw;
  background: #fff;
  box-sizing: border-box;
  border: 1px solid #e2e2e2;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.sideicon.icon-0 {
  bottom: 20vh;
}

.sideicon.icon-1 {

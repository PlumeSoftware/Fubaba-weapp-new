/* 搜索框及筛选栏样式 */
.ershoufang-filter-bar {
  background-color: var(--weui-BG-2);
}

/* 搜索栏样式 */
.ershoufang-filter-bar .search-container {
  padding: 9px;
  background-color: #ededed;
}

.ershoufang-filter-bar .search-container .fubaba-search-bar {
  border-radius: 4px;
  background-color: #fff;
  position: relative;
  padding: 4px;
  display: flex;
  box-sizing: border-box;
  -webkit-text-size-adjust: 100%;
  align-items: center;
}

.ershoufang-filter-bar .search-container .fubaba-search-bar .search-addon {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-right: 12px;
  padding-left: 6px;
}

.ershoufang-filter-bar .search-container .fubaba-search-bar .search-addon .search-city {
  font-size: 14px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
  white-space: nowrap;
  margin-left: 8px;
  position: relative;
  border: none;
}
.ershoufang-filter-bar .search-container .fubaba-search-bar .search-addon .search-city:before {
  content: "";
  width: 1px;
  height: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  background: #ced1d6;
  background-image: initial;
  background-position-x: initial;
  background-position-y: initial;
  background-size: initial;
  background-attachment: initial;
  background-origin: initial;
  background-clip: initial;
  background-color: rgb(206, 209, 214);
}
.ershoufang-filter-bar .search-container .fubaba-search-bar .search-addon .search-city .location {
  display: inline-flex;
  width: 16px;
  height: 16px;
  background-repeat: no-repeat;
  background-size: 16px 16px;
  margin-left: 12px;
  margin-right: 6px;
}

.ershoufang-filter-bar .search-container .fubaba-search-bar .weui-search-bar__form {
  border-radius: 4px;
  background-color: #fff;
  position: relative;
  flex: auto;
}

.ershoufang-filter-bar .search-container .fubaba-search-bar .weui-search-bar__form .weui-search-bar__box {
  padding-right: 0;
  border-radius: 4px;
  background-color: #fff;
  position: relative;
  padding-right: 32px;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.ershoufang-filter-bar .search-container .fubaba-search-bar .weui-search-bar__form .weui-search-bar__box .weui-icon-search {
  position: absolute;
  top: 50%;
  left: 8px;
  margin-top: -8px;
  color: var(--weui-FG-1);
  -webkit-mask-image: url(data:image/svg+xml,%3Csvg%20width%3D%2224%22%20height%3D%2224%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Cpath%20d%3D%22M16.31%2015.561l4.114%204.115-.848.848-4.123-4.123a7%207%200%2011.857-.84zM16.8%2011a5.8%205.8%200%2010-11.6%200%205.8%205.8%200%200011.6%200z%22%20fill-rule%3D%22evenodd%22%2F%3E%3C%2Fsvg%3E);
  mask-image: url(data:image/svg+xml,%3Csvg%20width%3D%2224%22%20height%3D%2224%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Cpath%20d%3D%22M16.31%2015.561l4.114%204.115-.848.848-4.123-4.123a7%207%200%2011.857-.84zM16.8%2011a5.8%205.8%200%2010-11.6%200%205.8%205.8%200%200011.6%200z%22%20fill-rule%3D%22evenodd%22%2F%3E%3C%2Fsvg%3E);
}

.ershoufang-filter-bar .search-container .fubaba-search-bar .weui-search-bar__form .weui-search-bar__box .weui-search-bar__input {
  padding: 8px 0;
  width: 100%;
  height: 1.14285714em;
  border: 0;
  font-size: 14px;
  line-height: 1.14285714em;
  box-sizing: content-box;
  background: transparent;
  caret-color: #07c160;
  color: rgba(0, 0, 0, 0.9);
}

/* 筛选栏样式 */
.ershoufang-filter-bar .tab-container {
  border-bottom: 1px solid #f0f0f0;
  z-index: 5;
}

.ershoufang-filter-bar .tab-container .kem__tab-bar {
  background: #fff;
  width: 100vw;
  height: 10.667vw;
  line-height: 10.667vw;
  z-index: 1;
}

.ershoufang-filter-bar .tab-container .kem__tab-bar .tab-arrow {
  margin-left: -1.6vw;
  list-style: none;
  margin: 0 6.4vw;
  display: flex;
}

.ershoufang-filter-bar .tab-container .kem__tab-bar .tab-arrow .tab-item {
  padding: 0 1.6vw;
  position: relative;
  list-style: none;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 1px;
  flex: 1 1;
  color: #222;
  font-size: 3.733vw;
  letter-spacing: 0;
  text-align: center;
}
.ershoufang-filter-bar .tab-container .kem__tab-bar .tab-arrow .tab-item:after {
  content: "";
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  width: 2.133vw;
  height: 2.133vw;
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: 2.133vw;
  background-image: url(data:image/png;base64,);
  display: block;
}
.kem__tab-bar ul .tab-item.active, .kem__tab-bar ul .tab-item.active-show, .kem__tab-bar ul .tab-item:active {
  color: #3072f6;
}

.kem__tab-bar ul .tab-item.active-show:after {
  background-image: url(data:image/png;base64,);
}
.active-show{
  color: #3072f6;
}


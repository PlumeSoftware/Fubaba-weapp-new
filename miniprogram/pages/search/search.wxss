.search-container {
  padding: 8px;
}

.main-container {
  background-color: var(--weui-BG-2);
}

.hot-search {
  margin-top: 0;
}

.search-history {
  display: flex;
  justify-content: space-between;
  align-items: center;

 
}
.title {
  font-size: 15px;
  font-weight: 700;
  flex: 1 1;
  flex-shrink: 0;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.icon-delete {
  width: 3.733vw;
  height: 4.267vw;
  background-size: 100%;
  flex-shrink: 0;
  /* background-image: url('@{images}/icon_delete.png'); */
}
.keyword-list {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.keyword-list .keyword-item{
  min-width: 22vw;
  height: 22px;
  margin-right: 6vw;
  margin-bottom: 12px;
  overflow: hidden;
}

.keyword-item p{
  flex: 0 1 auto;
  font-size: 12px;
  padding: 12px;
  color: #222;
  background-color: #f8f8f8;
  border-radius: .533vw;
}

.suggestion-list {

}
.sug-region {
  vertical-align: .8vw;
  height: 1.1875rem;
  width: 2.75rem;
  font-size: .75rem;
  color: #a0a3a5;
  letter-spacing: 0;
  line-height: 1.1875rem;
  text-align: center;
  display: inline-block;
  background: #f5f5f5;
  border-radius: .533vw;
  margin-right: .625rem;
}

.weui-cell__bd {
  overflow: hidden;
}

.region {
  color: #a8a8a8;
}

.ellipsis-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: auto;
}.weui-search-bar__form {
  position: relative;
  flex: auto;
  background-color: #fff;
  border-radius: 4px;
}
.weui-search-bar {
  position: relative;
  padding: 12px;
  display: flex;
  box-sizing: border-box;
  /* background-color: @weuiBgColorDefault; */
  -webkit-text-size-adjust: 100%;
  align-items: center;
}
.weui-search-bar__cancel-btn {
  display: block;
}
.weui-search-bar__label {
  display: none;
}

.weui-icon-search {
  width: 16px;
  height: 16px;
}
.weui-search-bar__box {
  position: relative;
  padding-left: 28px;
  padding-right: 32px;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}
.weui-search-bar__box .weui-icon-search {
  position: absolute;
  top: 50%;
  left: 8px;
  margin-top: -8px;
}

.weui-search-bar .weui-icon-search {
  width: 16px;
  height: 16px;
}
.weui-search-bar__box .weui-search-bar__input {
  /* padding: 8px 0; */
  width: 100%;
  height: 1.14285714em;
  border: 0;
  font-size: 14px;
  line-height: 1.14285714em;
  box-sizing: content-box;
  background: transparent;
  caret-color:#07c160;
  color: rgba(0, 0, 0, 0.9);
}
.search-history[_ngcontent-awy-c98] {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.weui-panel__hd {
  padding:  16px 16px 13px;
  color: var(--weui-FG-0);
  font-size: 15px;
  font-weight: 700;
  position: relative;
}
.weui-panel:first-child {
  margin-top: 0;
}
.weui-panel {
  background-color: #fff;
  position: relative;
  overflow: hidden;
}
/* .weui-media-box {
  padding: 16px;
  position: relative;
} */
.weui-panel {
  background-color: #fff;
  position: relative;
  overflow: hidden;
}
.weui-panel__bd {
  padding: 16px 16px 13px;
}
.weui-cells {
  margin-top: 8px;
  background-color: var(--weui-BG-2);
  line-height: 1.41176471;
  font-size: 17px;
  overflow: hidden;
  position: relative;
}
.weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  color: rgba(0, 0, 0, 0.1);
  transform-origin: 0 0;
  transform: scaleY(0.5);
  z-index: 2;
}
.weui-cell_access {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  color: inherit;
}
.weui-cell {
  padding: 16px;
  position: relative;
  display: flex;
  align-items: center;
}
.suggestion-list .sug-region {
  vertical-align: 0.8vw;
  height: 1.1875rem;
  width: 2.75rem;
  font-size: 0.75rem;
  color: #a0a3a5;
  letter-spacing: 0;
  line-height: 1.1875rem;
  text-align: center;
  display: inline-block;
  background: #f5f5f5;
  border-radius: 0.533vw;
  margin-right: 0.625rem;
}
.suggestion-list .weui-cell__bd {
  overflow: hidden;
}
.weui-cell__bd {
  flex: 1;
}
.suggestion-list .ellipsis-txt {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: auto;
}
.suggestion-list .region {
  color: #a8a8a8;
}
.weui-cell_access .weui-cell__ft {
  padding-right: 22px;
  position: relative;
}
.weui-cell__ft {
  text-align: right;
  color: rgba(0, 0, 0, 0.5);
}
.weui-cell_access .weui-cell__ft:after {
  content: " ";
  width: 12px;
  height: 24px;
  -webkit-mask-position: 0 0;
  mask-position: 0 0;
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  -webkit-mask-size: 100%;
  mask-size: 100%;
  background-color: currentColor;
  color: var(--weui-FG-2);
  -webkit-mask-image: url(data:image/svg+xml,%3Csvg%20width%3D%2212%22%20height%3D%2224%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Cpath%20d%3D%22M2.454%206.58l1.06-1.06%205.78%205.779a.996.996%200%20010%201.413l-5.78%205.779-1.06-1.061%205.425-5.425-5.425-5.424z%22%20fill%3D%22%23B2B2B2%22%20fill-rule%3D%22evenodd%22%2F%3E%3C%2Fsvg%3E);
  mask-image: url(data:image/svg+xml,%3Csvg%20width%3D%2212%22%20height%3D%2224%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Cpath%20d%3D%22M2.454%206.58l1.06-1.06%205.78%205.779a.996.996%200%20010%201.413l-5.78%205.779-1.06-1.061%205.425-5.425-5.425-5.424z%22%20fill%3D%22%23B2B2B2%22%20fill-rule%3D%22evenodd%22%2F%3E%3C%2Fsvg%3E);
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -12px;
}
.weui-media-box {
  padding: 12px;
  position: relative;
}
.weui-flex__item {
  flex: 1;
}
.weui-panel__hd:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  color: rgba(0, 0, 0, 0.1);
  transform-origin: 0 100%;
  transform: scaleY(0.5);
  left: 15px;
}
.weui-font{
  font-size: 15px;
}